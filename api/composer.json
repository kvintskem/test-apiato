{
  "name": "apiato/apiato",
  "description": "A flawless framework for building scalable and testable API-Centric Apps with PHP and Laravel.",
  "homepage": "http://apiato.io/",
  "support": {
    "issues": "https://github.com/apiato/apiato/issues",
    "source": "https://github.com/apiato/apiato"
  },
  "authors": [
    {
      "name": "Mahmoud Zalt",
      "email": "mahmoud@zalt.me"
    }
  ],
  "keywords": [
    "Apiato",
    "API",
    "Framework",
    "API framework",
    "API Starter",
    "Laravel API",
    "PHP",
    "Hello API",
    "Porto",
    "Porto SAP"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=7.3",
    "ext-intl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-openssl": "*",
    "ext-pdo": "*",
    "ext-tokenizer": "*",
    "adldap2/adldap2-laravel": "^6.0",
    "apiato/core": "*@dev",
    "asmiarowski/laravel-postgres": "^0.1.4",
    "bensampo/laravel-enum": "^1.26",
    "berkayk/onesignal-laravel": "^1.0",
    "darkaonline/l5-swagger": "5.7.*",
    "doctrine/dbal": "2.6.*",
    "elasticsearch/elasticsearch": "^7.7",
    "fico7489/laravel-eloquent-join": "^4.1",
    "fideloper/proxy": "~4.0",
    "guzzlehttp/guzzle": "^6.5",
    "jenssegers/agent": "^2.4",
    "kalnoy/nestedset": "~4.3.0",
    "laravel/framework": "5.6.38",
    "laravel/passport": "~5.0",
    "laravel/tinker": "~1.0",
    "lcobucci/jwt": "3.3.3",
    "maatwebsite/excel": "^3.1",
    "mews/purifier": "^3.2",
    "pda/pheanstalk": "^3.1",
    "phan/phan": "^3.1",
    "php-ews/php-ews": "~1.0",
    "predis/predis": "^1.1",
    "pvsv1984/secure-env-php": "^2.0",
    "smi2/phpclickhouse": "^1.3",
    "spatie/laravel-permission": "^2.9",
    "staudenmeir/eloquent-json-relations": "^1.1",
    "tymon/jwt-auth": "dev-develop",
    "vearutop/php-obscene-censor-rus": "^1.0",
    "wikimedia/composer-merge-plugin": "^1.3.1"
  },
  "require-dev": {
    "arcanedev/log-viewer": "4.5.4",
    "barryvdh/laravel-ide-helper": "2.8.*",
    "filp/whoops": "~2.0",
    "fzaninotto/faker": "~1.4",
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit": "~7.0"
  },
  "autoload": {
    "classmap": [
      "database/seeds",
      "database/factories"
    ],
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {}
  },
  "extra": {
    "laravel": {
      "dont-discover": [
        "*"
      ]
    },
    "merge-plugin": {
      "include": [
        "app/Ship/composer.json",
        "app/Containers/*/composer.json",
        "app/Containers/composer.json"
      ],
      "recurse": true,
      "replace": false,
      "merge-dev": true,
      "merge-extra": false,
      "merge-extra-deep": false,
      "merge-scripts": true
    }
  },
  "scripts": {
    "post-install-cmd": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
      "@php -r \"file_exists('.password') || copy('.password.example', '.password');\"",
      "@php artisan key:generate",
      "@php artisan jwt:secret",
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ],
    "post-update-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan ide-helper:generate",
      "@php artisan package:discover"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
